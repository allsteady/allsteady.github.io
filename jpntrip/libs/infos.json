			endPos.find( "." + item.ctg).append ( $( "<option value='"+ item.code+ "'>"+ item.name+ "</option>" ) );
		} else {
			startSelect.append ( $( "<option value='"+ item.code+ "'>"+ item.name+ "</option>" ) );
			endPos.append ( $( "<option value='"+ item.code+ "'>"+ item.name+ "</option>" ) );
		}
	}
});